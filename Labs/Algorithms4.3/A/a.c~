#include <stdio.h>

int main()
{
    char nums[50000] = { 0 };
    for (size_t i = 2; i * i < 50000; ++i)
    {
        if (!nums[i])
        {
            for (size_t j = i * i; j < 50000; j += i)
            {
                nums[j] = 1;
            }
        }
    }

    unsigned int cntr = 0;

    for (size_t i = 2; i < 50000; ++i)
    {
        if (!nums[i])
        {
            ++cntr;
        }
    }

    printf("%u\n", cntr);

    unsigned int n, a;

    scanf("%u", &n);
    
    for (size_t i = 0; i < n; ++i)
    {
        scanf("%u", &a);
        if (nums[a])
        {
            printf("NO\n");
        }
        else
        {
            printf("YES\n");
        }
    }
    return 0;
}
